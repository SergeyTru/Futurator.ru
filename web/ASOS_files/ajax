var mboxCurrent = mboxFactories.get('default').get('target-global-mbox',0);mboxCurrent.setOffer(new mboxOfferAjax(''));mboxCurrent.getOffer().setOnLoad(function() {
(function() {

	//window.console && window.location.href.indexOf('mboxDebug') !== -1 && console.log("1: Product OOS Mbox (POC)");

	var isRequireOnProduct = function() {
		return window.location.href.indexOf("/prd/") !== -1
			&& document.querySelector('html').className.indexOf('MasterLayout') !== -1
			&& window.location.href.indexOf("ttqa=oos-") !== -1;
	}();

	if (isRequireOnProduct) {

		function createMbox(mboxName) {
			//window.console && window.location.href.indexOf('mboxDebug') !== -1 && console.log('Creating dynamic mbox', mboxName);
			if(typeof window.mboxDefine !== 'undefined') {
				var mbox = document.createElement("div");
				mbox.id = mboxName;
				mbox.className = 'mboxDefault';
				mbox.style.visibility = 'hidden';
				mbox.style.display = 'none';
				document.body.appendChild(mbox);
				mboxDefine(mboxName, mboxName);
				mboxUpdate(mboxName);
			}
		}

		function putInTest(ev, obj) {
			return (
				(typeof obj.IsInStock !== 'undefined' && !obj.IsInStock)
					|| 
				(window.location.href.toLowerCase().indexOf("&mrch=t1012") !== -1)
			);
		}
		
		require(["$"], function($) {
			$(window).on('tracking.productPage.pageLoad', function(ev, obj) {
				//window.console && console.log("putInTest(ev, obj): ", putInTest(ev, obj));
				if(putInTest(ev, obj)) {
					//window.console && window.location.href.indexOf('mboxDebug') !== -1 && console.log("createMbox(product-oos-mbox)");
					createMbox('product-oos-mbox');
				}
			});
		});
	
	}

}());
});mboxCurrent.loaded();mboxFactories.get('default').get('target-global-mbox',0).cancelTimeout();mboxFactories.get('default').get('target-global-mbox',0).setOffer(new mboxOfferDefault()).show();