function s_pgicq(){var a=window,k=a.s_giq,q,r,n;if(k)for(q=0;q<k.length;q++)r=k[q],n=s_gi(r.oun),n.setAccount(r.un),n.setTagContainer(r.tagContainerName);a.s_giq=0}s_pgicq();

// Get bag details from asosbasket cookie
var asosBagItems = function (){
    try{
        if( s.Util.cookieRead('asosbasket') !== '' && s.Util.cookieRead('asosbasket') !== undefined ){
            var basketList = s.Util.cookieRead('asosbasket').split('&');
            var basketObj = {};
            for(var j = 0; j < basketList.length; j ++){
                basketObj[ basketList[j].split('=')[0] ] = basketList[j].split('=')[1];
            }
            if(typeof Navigation !== 'undefined') {
                var response = basketObj['basketitemcount']  + '|' + basketObj['basketitemtotalretailprice'] + '|' + (Navigation.Currency != undefined ? Navigation.Currency : 'N/A');
                //window.console && console.log(response);
                return response;
            } else {
                return false;
            }
        }
    }
    catch(err){
        window.console && console.warn(err);
    }
};
