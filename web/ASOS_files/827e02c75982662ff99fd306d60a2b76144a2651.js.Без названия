s.overrideLinkTrack = function (linkTrackVarsValue, linkTrackEventsValue) { 
    // Function used to change track variable values on a unique event
    return {
        'linkTrackVars': s.apl(s.linkTrackVars, linkTrackVarsValue, ',', 1),
        'linkTrackEvents': s.apl(s.linkTrackEvents, linkTrackEventsValue, ',', 1)
    };
}
