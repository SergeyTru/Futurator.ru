function s_doPlugins(s) {
    /* Disable Target Hit-Stitching */
    //s.supplementalDataID = "";
    
    /* OPTIONAL Visitor ID Present? */
    s.prop29 = (typeof (Visitor) != "undefined" ? "VisitorAPI Present" : "VisitorAPI Missing");

    /* Search Passes (search results only) - 12-08-2015 */
    //s.prop43 = getSearchPassesString();
    
    /* TEMP BACKUP FOR CASE SENSITIVE - External Campaign Tracking */
    //s.eVar12 = s.Util.getQueryParam('affId');
    //if (s.Util.getQueryParam('affId')) {
    //    s.events = s.events + ",event6"
    //}

    /* External Campaign Stacking */
    s.eVar35 = s.crossVisitParticipation(s.eVar12, 's_ev35', '30', '5', '>', '', 1);

    /* Stock Service Unavailable */
    if(typeof(trackStockServicedown) !== 'undefined' && trackStockServicedown == 'true') { 
        s.events = s.apl(s.events,"event125",",",1); 
       }
    if(typeof(trackStockServicedown) !== 'undefined' && trackStockServicedown == 'false' && s.events.indexOf('event125') > 0) {
        s.events = s.events.replace(/,event125/, "");
    }
  
    /* Checkout Stock unavailable (in stock but not full # of units) */
    if(typeof(trackOutOfStock) !== 'undefined' && trackOutOfStock == 'true') { 
        s.events = s.apl(s.events,"event107",",",1);
    }
    if(typeof(trackOutOfStock) !== 'undefined' && trackOutOfStock == 'false' && s.events.indexOf('event107') > 0) { 
        s.events = s.events.replace(/,event107/, "");
    }

    /* Stock API Unavailable Tracking */
    if(typeof(Checkout) !== 'undefined' && typeof(Checkout.IsStockApiDown) !== 'undefined' && Checkout.IsStockApiDown === true) {
        s.events = s.apl(s.events,"event125",",",1);
    }
    
    /* Internal Search Stacking */
    if (s.events.indexOf("event3") >= 0 || s.events.indexOf("event4") >= 0) {
        s.eVar3 = s.crossVisitParticipation(s.eVar25, 's_ev3', '30', '5', '>', '', 1);
    }

    /* Visitor Status */
    s.eVar17 = s.getNewRepeat(365);
    s.eVar1 = s.getVisitNum();
    s.prop11 = window.location.href;
    s.prop10 = s.getPreviousValue(s.pageName, 'gpv_p10', '');
    s.eVar47 = s.getPreviousValue(s.eVar6, 'gpv_p11', '');

   /* Set Custom Product View Event */
    if(s.events.indexOf("prodView") >= 0){
        s.events = s.apl(s.events,"event1",",",1);
    }
    
    s.prop34 = s.getPercentPageViewed(s.prop10); //get array of data on prev page % viewed
    /* Set custom time stamp */
    CalculateCustomTime();

    // Reference global window method instead of contained
    s.eVar4 = window.analyticsGetPageProperties();

    analyticsGetNewCategoryPage = function() {
        if (typeof DataDictionary !== 'undefined' && typeof DataDictionary.Category !== 'undefined' && (DataDictionary.Category.NewCategory === "True" || DataDictionary.Category.NewCategory === true)) {
            return "new category page";
        } else {
            return "";
        }
    }

    analyticsGetCategoryPageErrorType = function() {
        if (typeof DataDictionary !== 'undefined' && typeof DataDictionary.Category !== 'undefined' && DataDictionary.Category.FailType === "Error") {
            return "error loading page";
        } else {
            return "";
        }
    }

    analyticsGetCategoryPageRedirect = function(a, b) {
        if (s.Util.getQueryParam(a).length > 0) {
            if (s.Util.getQueryParam(b).length > 0) {
                return s.Util.getQueryParam(a) + "|" + s.Util.getQueryParam(b);
            }
            return s.Util.getQueryParam(a);
        }
    }

    /* Replatformed Category Page Redirect Tracking */
    s.prop58 = analyticsGetCategoryPageRedirect("cr", "ocid");
    if (s.Util.getQueryParam("cr").length > 0) {
        s.events = s.apl(s.events,"event131",",",1);
    }

    /* Replatformed Category Page Error Type */
    s.eVar77 = analyticsGetCategoryPageErrorType();

    /* Replatformed Category Page Error Event */
    if (typeof DataDictionary !== 'undefined' && typeof DataDictionary.Category !== 'undefined' && DataDictionary.Category.FailType !== "") {
        s.events = s.apl(s.events,"event92",",",1);
    }

    /* Replatformed Category Page Impressions */
    s.eVar78 = analyticsGetNewCategoryPage();
    
    /* Basket details on page load and A2B */
    s.eVar70 = asosBagItems();    
    /*
        Performance metrics - Rob Brown
        Sends the time to first byte and domready for the current page
        Sends page load for the previous page and stores the current page laod for sending on the subsequent page load
        Page load is always one page behind as Analytics is triggered before page load
    */
    if(typeof(window.performance) !== 'undefined'){
        var navApi = window.performance.timing;
        s.eVar29 = navApi.responseStart - navApi.navigationStart; //first byte
        s.eVar30 = navApi.domContentLoadedEventEnd - navApi.navigationStart; //dom ready

        s.events = s.apl(s.events,"event128=" + s.eVar30,",",1);

        var previousPageLoad = s.Util.cookieRead('_s_pl');
        
        if(previousPageLoad != "" && s.prop10 != ""){
            s.prop46 = previousPageLoad;
            s.Util.cookieWrite('_s_pl','');
        }

        var expires = new Date();
        expires.setTime(expires.getTime() + 31536000000);

        if(navApi.loadEventStart){
            s.Util.cookieWrite('_s_pl', navApi.loadEventStart - navApi.navigationStart, expires);
        }
        else{
            $(window).load(function(navApi, expires){
                s.Util.cookieWrite('_s_pl', navApi.loadEventStart - navApi.navigationStart, expires);
            });
        }
    }

    /* 
    GIFT CARD shiz (stop looking!)
    */
    try {
    
        if(typeof giftcardAnalytics !== 'undefined'){
    
            if(giftcardAnalytics['giftcardStatus'] === 'error'){
                //giftcardAction|giftcardStatus|errorCode|responseTime
                s.prop65 = giftcardAnalytics['giftcardAction'] + '|' + giftcardAnalytics['giftcardStatus'] + '|' + giftcardAnalytics['errorCode'] + '|' + giftcardAnalytics['responseTime'];
            } else {
                //giftcardAction|giftcardStatus|responseTime
                s.prop67 = giftcardAnalytics['giftcardAction'] + '|' + giftcardAnalytics['giftcardStatus']  + '|' + giftcardAnalytics['responseTime'];
            }
        }
    
    }
    catch(err){
        window.console && console.error(err);
    }    

    try {
        if(typeof Order !== 'undefined'){
            if(Order['GiftVoucherAdded'] === true){
                s.prop63 = ('GiftVoucherStyleId' in Order) ? Order['GiftVoucherStyleId'] : 'Voucher Style ID Not Set';
                s.prop64 = ('GiftVoucherId' in Order) ? Order['GiftVoucherId'] : 'Voucher ID Not Set';
                if('GiftVoucherValue' in Order){
                    if(Order['GiftVoucherValue'] !== '' && Order['GiftVoucherValue'] !== '0' && Order['GiftVoucherValue'] !== '0.00'){
                        s.events = s.apl(s.events,"event136=" + Order['GiftVoucherValue'],",",1);
                    }
                }
            } 
        }
    }
    catch(err){
        window.console && console.error(err);
    }
    /*
    /GIFT CARD 
    */

    /*Set the category ID value
    AJones 13-09-2016 commented out and moved in page view tag
   if (s.Util.getQueryParam("cid") !== '') {
        s.eVar61 = s.eVar60 = s.Util.getQueryParam("cid");
    }
    */
        
}
