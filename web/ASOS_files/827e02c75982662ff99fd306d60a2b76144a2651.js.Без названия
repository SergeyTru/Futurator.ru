// Move Analytics Get Properties method to global window scope for IE refinement issue (27/06/2016)
window.analyticsGetPageProperties = function() {
    var output = '';
    if ((typeof Search !== 'undefined' && Search.PageTitle === "FailedSearch") || (typeof DataDictionary !== 'undefined' && typeof DataDictionary.Category !== 'undefined' && DataDictionary.Category.FailType !== "")) {
        output = ''; // Failed Search Pages
    } else if (typeof DataDictionary !== 'undefined' && typeof DataDictionary.ProductList !== 'undefined') {
        output = DataDictionary.ProductList.GridSize + "|" + (DataDictionary.ProductList.PageNumber + 1) + "|" + DataDictionary.ProductList.PageSize + "|" + DataDictionary.ProductList.ProductDepth + "|" + DataDictionary.ProductList.TotalStyles; // New Data Dictionary format
    } else if (typeof PageProperties !== 'undefined') {
        output = PageProperties.GridSize + "|" + PageProperties.PageNumber + "|" + PageProperties.PageSize + "|" + PageProperties.ProductDepth + "|" + PageProperties.TotalStyles; // Old Data Dictionary format
    } else if (s.Util.getQueryParam("pge").length > 0 && s.Util.getQueryParam("pgesize").length > 0 && s.Util.getQueryParam("totalstyles").length > 0 && s.Util.getQueryParam("gridsize").length > 0) {
        output = s.Util.getQueryParam("gridsize") + "|" + (parseInt(s.Util.getQueryParam("pge")) + 1).toString() + "|" + s.Util.getQueryParam("pgesize") + "|" + (s.Util.getQueryParam("gridsize") * s.Util.getQueryParam("pgesize")).toString() + "|" + s.Util.getQueryParam("totalstyles"); // From URL parameters on Product Pages
    }
    return output;
}


/* WARNING: Changing any of the below variables will cause drastic
changes to how your visitor data is collected.  Changes should only be
made when instructed to do so by your account manager.*/

if (host != 'undefined' && host != null) {
    if (host.indexOf(".fr") > -1) {
        s.trackingServer = "metric.asos.fr"
        s.trackingServerSecure = "metrics.asos.fr"
    } else if (host.indexOf(".de") > -1) {
        s.trackingServer = "metric.asos.de"
        s.trackingServerSecure = "metrics.asos.de"
    } else {
        s.trackingServer = "metric.asos.com"
        s.trackingServerSecure = "metrics.asos.com"
    }
}

