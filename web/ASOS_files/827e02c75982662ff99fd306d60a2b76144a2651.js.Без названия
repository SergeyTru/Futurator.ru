CalculateCustomTime();

function CalculateCustomTime() {
    var d = new Date();
    var currentTimeZone = -d.getTimezoneOffset() / 60;
    var a = String(currentTimeZone).split('.');
    if (a.length >= 1) {
        if (a[0] < 10) {
            if (a[0] < 0) {
                currentTimeZone = '-0' + String(-a[0]);
            } else {
                currentTimeZone = '0' + String(a[0]);
            }
        }
        if (a.length == 2) {
            currentTimeZone = currentTimeZone + (parseInt(a[1]) * 60 / 10);
        } else {
            currentTimeZone = currentTimeZone + '00'
        }
    }

    var tempstr = " GMT"
    if (currentTimeZone >= 0) {
        tempstr = tempstr + '+';
    }

    var CurrentDate = d.getDate();
    if (CurrentDate < 10) {
        CurrentDate = '0' + String(CurrentDate);
    }
    var CurrentMonth = d.getMonth() + 1;
    if (CurrentMonth < 10) {
        CurrentMonth = '0' + String(CurrentMonth);
    }
    var CurrentHrs = d.getHours();
    if (CurrentHrs < 10) {
        CurrentHrs = '0' + String(CurrentHrs);
    }

    var CurrentMin = d.getMinutes();
    if (CurrentMin < 10) {
        CurrentMin = '0' + String(CurrentMin);
    }

    var output = CurrentDate + '/' + CurrentMonth + '/' + d.getFullYear() + ' ' + CurrentHrs + ':' + CurrentMin + tempstr + currentTimeZone;
    s.prop5 = output;
}

